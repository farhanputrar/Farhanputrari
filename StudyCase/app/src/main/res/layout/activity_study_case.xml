<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.user.studycase.StudyCase">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation=""

    <TextView
        android:id="@+id/Judul1"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Makan Malam"
        android:textSize="60px"
        android:textAlignment="center"
        tools:layout_editor_absoluteX="91dp"
        tools:layout_editor_absoluteY="68dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        />

    <TextView
        android:id="@+id/Judul2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/Judul2"
        android:text="dengan Milea"
        android:textSize="35px"
        android:textAlignment="center"
        android:layout_centerHorizontal="true"
        tools:layout_editor_absoluteX="44dp"
        tools:layout_editor_absoluteY="116dp" />

    <EditText
        android:id="@+id/Makanan"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textMultiLine"
        android:layout_below="@+id/Judul1"
        android:hint="Menu makanan"
        android:inputType="textPersonName"
        tools:layout_editor_absoluteX="85dp"
        tools:layout_editor_absoluteY="177dp" />

    <EditText
        android:id="@+id/Jumlah"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:ems="10"
        android:inputType="textMultiLine"
        android:hint="jumlah"
        android:layout_alignStart="@+id/Judul2"
        tools:layout_editor_absoluteX="84dp"
        tools:layout_editor_absoluteY="265dp" />

    <Button
        android:id="@+id/eatbus"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="EATBUS"
        android:layout_alignParentStart="true"
        android:background="@color/colorAccent"
        android:onClick="selectEatbus"
        android:text="@string/pilihan1"
        tools:layout_editor_absoluteX="85dp"
        tools:layout_editor_absoluteY="366dp" />

    <Button
        android:id="@+id/abnormal"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ABNORMAL"
        android:background="@color/colorAccent"
        android:onClick="selectAbnormal"
        tools:layout_editor_absoluteX="204dp"
        tools:layout_editor_absoluteY="366dp" /><![CDATA[

    "
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent" />

]]>
</LinearLayout>

</android.support.constraint.ConstraintLayout>
